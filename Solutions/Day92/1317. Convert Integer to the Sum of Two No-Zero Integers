class Solution {
    public int[] getNoZeroIntegers(int n) {
        for(int i = 1 ; i < n ; i ++){
            int b = n-i;
            if(valid(i)&&valid(b)){
                return new int[]{i,b};
            }
        }
        return new int[]{};
    }
    static boolean valid(int a ){
        while(a>0){
            if(a%10==0) return false;
            a/=10;
        }
        return true ;
    }
}
