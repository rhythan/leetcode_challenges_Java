class Solution {
    public int divisorSubstrings(int num, int k) {
        String s = String.valueOf(num);
        int cnt = 0 ;
        for(int i = 0 ; i <= s.length() - k ; i ++){
            String substr = s.substring(i,i+k);
            int subnums = Integer.parseInt(substr);
            if(subnums!=0 && num%subnums==0 ){
                cnt++;
            }
        }
        return cnt;
    }
}
