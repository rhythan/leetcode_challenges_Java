class Solution {
    public int maxFrequencyElements(int[] nums) {
        int max = 0 ,count = 0 ;
        Map<Integer,Integer> map = new HashMap<>();
        for(int num : nums){
            map.put(num,map.getOrDefault(num,0)+1);
        }
        for (Map.Entry<Integer, Integer> entry : map.entrySet()) {
            if(entry.getValue()>max){
                max=entry.getValue();
                count=0;
            }
            if(entry.getValue()==max){
                count++;
            }
            
        }
        return max*count;
    
}



    }
