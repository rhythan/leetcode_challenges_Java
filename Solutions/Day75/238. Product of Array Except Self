class Solution {
    public int[] productExceptSelf(int[] nums) {
        int[] arr = new int[nums.length];
        arr[0]=1;
        for(int i = 1 ; i < arr.length ; i ++){
            arr[i]=arr[i-1]*nums[i-1];
        }
        int suffix = 1 ;
        for(int j = nums.length-1 ; j >= 0 ; j--){
            arr[j]=arr[j]*suffix;
            suffix*=nums[j];
        }
        
    
        return arr;
    }
       
}
