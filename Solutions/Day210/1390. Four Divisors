class Solution {
    public int sumFourDivisors(int[] nums) {
        int ans = 0 ;
        for(int num  : nums){
            ans+=validNum(num);
        }
        return ans;
    }
    int validNum(int num){
        int cnt = 0 , sum = 0 ;
        for(int i = 1 ; i*i <= num ; i ++){
            if(num%i==0){
                if(i==num/i){
                    sum+=i;
                    cnt++;
                }
                else{
                    sum+=i+(num/i);
                    cnt+=2;
                }
                if(cnt>4) return 0;

            } 
            
        }
       
        return cnt==4?sum:0;
        

    }
    
}
