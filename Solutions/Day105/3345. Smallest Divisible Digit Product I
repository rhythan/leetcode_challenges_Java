class Solution {
    public int smallestNumber(int n, int t) {

        while (true) {
            if (digit(n, 1) % t == 0) {
                return n;
            }
            n++;
        }

    }

    static int digit(int n, int p) {
        while (n > 0) {
            p *= n % 10;
            n /= 10;
        }
        return p;
    }
}
