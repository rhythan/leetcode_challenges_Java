class Solution {
    public boolean reorderedPowerOf2(int n) {
        int[] targetcounts = digitcount(n);
        for(int i = 0 ; i < 31 ; i ++){
            int[] powercounts = digitcount(1<<i);
            if(check(targetcounts,powercounts)){
                return true ;
                
            }
        }
        return false ;
    }
    static int[] digitcount(int num){
        int[] count = new int[10];
        while(num>0){
            count[num%10]++;
            num/=10;
        }
        return count;
    }
    static boolean check(int[] x , int[] y){
        for(int i = 0 ; i < 10 ; i ++){
            if(x[i]!=y[i]){
                return false;
            }
        }
        return true;
    }
}
