class Solution {
    public int findFinalValue(int[] nums, int original) {
        
        // for (int i = 0; i < nums.length; i++) {
        //     for (int j = 0; j < nums.length; j++) {
        //         if (nums[j] == original) {
        //             original *= 2;
        //         }
        //     }
        // }
        // return original;
        HashSet<Integer> set = new HashSet<>();
        for(int num : nums){
            set.add(num);

        }
        while(true){
            if(set.contains(original)){
                original*=2;
            }
            else{
                break;
            }
        }
        return original;
    }
}
