class Solution {
    public int countSymmetricIntegers(int low, int high) {
        int cnt = 0;
        for (int i = low; i <= high; i++) {
            String s = i+"";
            
            if (s.length() % 2 == 0) {
                int mid = s.length() / 2;
                int leftSum = 0, rightSum = 0;
                
                for (int j = 0; j < mid; j++) {
                    leftSum += s.charAt(j) - '0';
                }
                
                for (int j = mid; j < s.length(); j++) {
                    rightSum += s.charAt(j) - '0';
                }
                
                if (leftSum == rightSum) {
                    cnt++;
                }
            }
        }
        return cnt;
    }
}
