class Solution {
    public String reverseWords(String s) {
        String[] words = s.trim().split("\\s+");
        StringBuilder ss = new StringBuilder();
        for(int i = words.length-1 ; i >= 0 ; i --){
            if(i!=0){
                ss.append(words[i]).append(" ");
            }
            else{
                ss.append(words[i]);
            }
            
        }
        return ss.toString();
    }
}
