class Solution {
    public String largestOddNumber(String num) {
        int end = num.length()-1 ;
        for(int i = num.length()-1 ; i >= 0 ; i --){
            int digit = num.charAt(i)-'0';
            if(digit%2!=0){
                break;
            }
            end--;
        }
        
        StringBuilder sb = new StringBuilder();
        for(int i = 0 ; i <= end ; i ++){
            sb.append(num.charAt(i));
        }
        return sb.toString();
    }
}
