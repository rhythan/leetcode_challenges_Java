class Solution {
    public int minOperations(int[] nums) {
        int n = nums.length, num = 0 , g = 0;

        for (int i = 0; i < nums.length; i++) {
            if (nums[i] == 1)
                num++;
            g = gcd(g, nums[i]);
        }
        if (num > 0)
            return n - num;
        if (g > 1)
            return -1;
        int minLen = n;
        for (int i = 0; i < n; i++) {
            int cur = 0;
            for (int j = i; j < n; j++) {
                cur = gcd(cur, nums[j]);
                if (cur == 1) {
                    minLen = Math.min(minLen, j - i + 1);
                    break;
                }
            }
        }
        return minLen + n - 2;

    }

    static int gcd(int a, int b) {
        while (b != 0) {
            int tem = b;
            b = a % b;
            a = tem;
        }
        return a;
    }
}
